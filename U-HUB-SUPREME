Fluent:Notify({Title = "U-HUB SUPREME", Content = "‡πÇ‡∏´‡∏•‡∏î‡∏£‡∏∞‡∏ö‡∏ö‡∏ü‡∏≤‡∏£‡πå‡∏°‡πÅ‡∏•‡∏∞‡πÅ‡∏Å‡πâ‡∏•‡∏≤‡∏Å‡∏°‡∏∑‡∏≠‡∏ñ‡∏∑‡∏≠‡πÄ‡∏™‡∏£‡πá‡∏à‡∏™‡∏¥‡πâ‡∏ô!", Duration = 5})


-- [[ U-HUB CORE ENGINE : NO UI VERSION ]]
-- ‡∏™‡∏≥‡∏´‡∏£‡∏±‡∏ö‡∏ô‡πâ‡∏≠‡∏á‡∏´‡∏ô‡∏∂‡πà‡∏á‡πÄ‡∏≠‡∏≤‡πÑ‡∏õ‡∏õ‡∏£‡∏∞‡∏Å‡∏≠‡∏ö‡πÉ‡∏™‡πà UI ‡∏ï‡∏±‡∏ß‡πÄ‡∏≠‡∏á‡∏Ñ‡∏£‡∏±‡∏ö

-- [[ 1. CONFIGURATION TABLE ]]
-- ‡∏´‡∏ô‡∏∂‡πà‡∏á‡∏™‡∏±‡πà‡∏á‡πÄ‡∏õ‡∏¥‡∏î-‡∏õ‡∏¥‡∏î‡∏ú‡πà‡∏≤‡∏ô‡∏õ‡∏∏‡πà‡∏°‡πÉ‡∏ô UI ‡πÇ‡∏î‡∏¢‡πÅ‡∏Å‡πâ‡∏Ñ‡πà‡∏≤‡πÉ‡∏ô‡∏ï‡∏≤‡∏£‡∏≤‡∏á‡∏ô‡∏µ‡πâ‡πÑ‡∏î‡πâ‡πÄ‡∏•‡∏¢
_G.U_HUB_CORE = {
    -- Aimbot
    AimbotEnabled = false,
    TeamCheck = false,
    AimPart = "Head",
    Sensitivity = 0.2, -- 0.1 ‡∏ñ‡∏∂‡∏á 1
    
    -- FOV
    FovEnabled = true,
    FovRadius = 150,
    FovColor = Color3.fromRGB(0, 255, 255),
    
    -- ESP
    EspEnabled = false,
    BoxEnabled = false,
    NameEnabled = false,
    EspColor = Color3.fromRGB(255, 0, 0)
}

-- [[ 2. VARIABLES & SERVICES ]]
local Camera = workspace.CurrentCamera
local Players = game:GetService("Players")
local LocalPlayer = Players.LocalPlayer
local RunService = game:GetService("RunService")
local UserInputService = game:GetService("UserInputService")

-- ‡∏™‡∏£‡πâ‡∏≤‡∏á‡∏ß‡∏á‡∏Å‡∏•‡∏° FOV
local FOVCircle = Drawing.new("Circle")
FOVCircle.Thickness = 1
FOVCircle.NumSides = 100
FOVCircle.Filled = false
FOVCircle.Transparency = 0.8

-- [[ 3. AIMBOT LOGIC ]]
local function GetClosestPlayer()
    local Target = nil
    local Distance = _G.U_HUB_CORE.FovRadius
    for _, v in pairs(Players:GetPlayers()) do
        if v ~= LocalPlayer and v.Character and v.Character:FindFirstChild(_G.U_HUB_CORE.AimPart) and v.Character:FindFirstChild("Humanoid") and v.Character.Humanoid.Health > 0 then
            if _G.U_HUB_CORE.TeamCheck and v.Team == LocalPlayer.Team then continue end
            
            local ScreenPos, OnScreen = Camera:WorldToViewportPoint(v.Character[_G.U_HUB_CORE.AimPart].Position)
            if OnScreen then
                local MouseLoc = UserInputService:GetMouseLocation()
                local MouseDist = (Vector2.new(MouseLoc.X, MouseLoc.Y) - Vector2.new(ScreenPos.X, ScreenPos.Y)).Magnitude
                if MouseDist < Distance then
                    Distance = MouseDist
                    Target = v
                end
            end
        end
    end
    return Target
end

-- [[ 4. ESP SYSTEM (BOX & NAME) ]]
local function CreateESP(Player)
    local Box = Drawing.new("Square")
    local Name = Drawing.new("Text")
    
    Box.Thickness = 1
    Box.Filled = false
    Name.Size = 16
    Name.Center = true
    Name.Outline = true

    local Connection
    Connection = RunService.RenderStepped:Connect(function()
        if Player.Character and Player.Character:FindFirstChild("HumanoidRootPart") and Player.Character.Humanoid.Health > 0 and Player ~= LocalPlayer then
            local RootPart = Player.Character.HumanoidRootPart
            local Pos, OnScreen = Camera:WorldToViewportPoint(RootPart.Position)
            
            if OnScreen and _G.U_HUB_CORE.EspEnabled then
                -- BOX ESP
                if _G.U_HUB_CORE.BoxEnabled then
                    local SizeX = 1000 / Pos.Z
                    local SizeY = 1500 / Pos.Z
                    Box.Size = Vector2.new(SizeX, SizeY)
                    Box.Position = Vector2.new(Pos.X - SizeX / 2, Pos.Y - SizeY / 2)
                    Box.Color = _G.U_HUB_CORE.EspColor
                    Box.Visible = true
                else Box.Visible = false end

                -- NAME ESP
                if _G.U_HUB_CORE.NameEnabled then
                    Name.Text = Player.Name
                    Name.Position = Vector2.new(Pos.X, Pos.Y - (1600 / Pos.Z) / 2 - 20)
                    Name.Color = Color3.fromRGB(255, 255, 255)
                    Name.Visible = true
                else Name.Visible = false end
            else
                Box.Visible = false
                Name.Visible = false
            end
        else
            if not Player.Parent then -- ‡∏ñ‡πâ‡∏≤‡∏ú‡∏π‡πâ‡πÄ‡∏•‡πà‡∏ô‡∏≠‡∏≠‡∏Å‡πÄ‡∏Å‡∏° ‡πÉ‡∏´‡πâ‡∏•‡∏ö‡πÄ‡∏™‡πâ‡∏ô‡∏ó‡∏¥‡πâ‡∏á
                Box:Remove()
                Name:Remove()
                Connection:Disconnect()
            end
            Box.Visible = false
            Name.Visible = false
        end
    end)
end

-- ‡∏£‡∏±‡∏ô ESP ‡∏™‡∏≥‡∏´‡∏£‡∏±‡∏ö‡∏ó‡∏∏‡∏Å‡∏Ñ‡∏ô
for _, p in pairs(Players:GetPlayers()) do CreateESP(p) end
Players.PlayerAdded:Connect(CreateESP)

-- [[ 5. MAIN ENGINE LOOP ]]
RunService.RenderStepped:Connect(function()
    -- ‡∏≠‡∏±‡∏õ‡πÄ‡∏î‡∏ï‡∏ß‡∏á‡∏Å‡∏•‡∏° FOV
    FOVCircle.Visible = _G.U_HUB_CORE.FovEnabled
    FOVCircle.Radius = _G.U_HUB_CORE.FovRadius
    FOVCircle.Color = _G.U_HUB_CORE.FovColor
    FOVCircle.Position = Vector2.new(UserInputService:GetMouseLocation().X, UserInputService:GetMouseLocation().Y)

    -- ‡∏£‡∏∞‡∏ö‡∏ö Aimbot (‡∏•‡πá‡∏≠‡∏Ñ‡πÄ‡∏°‡∏∑‡πà‡∏≠‡∏Ñ‡∏•‡∏¥‡∏Å‡∏Ç‡∏ß‡∏≤‡∏Ñ‡πâ‡∏≤‡∏á)
    if _G.U_HUB_CORE.AimbotEnabled and UserInputService:IsMouseButtonPressed(Enum.UserInputType.MouseButton2) then
        local Target = GetClosestPlayer()
        if Target then
            local TargetPos = Camera:WorldToViewportPoint(Target.Character[_G.U_HUB_CORE.AimPart].Position)
            local MousePos = UserInputService:GetMouseLocation()
            mousemoverel(
                (TargetPos.X - MousePos.X) * _G.U_HUB_CORE.Sensitivity,
                (TargetPos.Y - MousePos.Y) * _G.U_HUB_CORE.Sensitivity
            )
        end
    end
end)

print("U-HUB Core Loaded! ‡πÄ‡∏ä‡∏∑‡πà‡∏≠‡∏°‡∏ï‡πà‡∏≠‡∏Å‡∏±‡∏ö UI ‡∏Ç‡∏≠‡∏á‡∏´‡∏ô‡∏∂‡πà‡∏á‡πÑ‡∏î‡πâ‡πÄ‡∏•‡∏¢‡∏Ñ‡∏£‡∏±‡∏ö")
